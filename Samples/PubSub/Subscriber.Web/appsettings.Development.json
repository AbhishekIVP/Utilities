{
  "$schema": "../../../Resources/edm.schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Dapr": {
    "DefaultSecretStore": "local"
  },
  "PubSub": {
    "Name": "",
    "QueueProvider": "Redis",
    "IsMultiTenant": true,
    "TopicRouteMappings": [
      {
        "QueueName": [
          "edmqueue1",
          "edmqueue3"
        ],
        "MethodRoute": "ProcessMessage"
      },
      {
        "QueueName": [
          "edmqueue2"
        ],
        "MethodRoute": "ProcessMessage2"
      }
    ]
  }
}